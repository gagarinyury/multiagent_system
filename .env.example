# API ключи
ANTHROPIC_API_KEY=your_anthropic_api_key
OPENAI_API_KEY=your_openai_api_key

# Настройки базы данных
DB_PATH=data/db.sqlite

# Настройки логирования
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# Настройки агентов (включены по умолчанию)
ENABLE_PLANNER=true
ENABLE_ARCHITECT=true
ENABLE_CODER=true
ENABLE_REVIEWER=true
ENABLE_TESTER=true
ENABLE_DOCUMENTER=true

# Настройки моделей по умолчанию
DEFAULT_CLAUDE_MODEL=claude-3-sonnet-20240229
DEFAULT_GPT_MODEL=gpt-4-turbo

# Настройки кэширования
ENABLE_CACHE=true
CACHE_TTL=3600  # Время жизни кэша в секундах (1 час)